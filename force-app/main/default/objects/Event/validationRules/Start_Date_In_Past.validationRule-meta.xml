<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_In_Past</fullName>
    <active>true</active>
    <errorConditionFormula>ActivityDateTime &lt;  $System.OriginDateTime</errorConditionFormula>
    <errorMessage>Start Date cannot be prior to current date/time</errorMessage>
</ValidationRule>
